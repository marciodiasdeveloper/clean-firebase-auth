  "paths": {
    "/token": {
      "post": {
        "tags": [
          "User Access Token"
        ],
        "summary": "[UAT] Obtain access token for a user",
        "description": "Get access token for a user",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "name": "host",
            "in": "header",
            "description": "Host Header example: foobar.com",
            "required": true
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "username",
                  "password"
                ],
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string",
                      "default": "eyJhbGciOiJSUzI1NiIsInR5cCIgOiAiSldUIiwia2lkIiA6ICJ1dVN6aVhmaWFpbXlmRmhacHRkWTByRGFBTTdwcENoY0NDTHhTLVJNTlRNIn0..WNAqseZE0IFeFHESpPLHgRibzNqgJw3KIUg8qZal7Xe-DPlPzeK0FoiYA_SQIjZTK3qnRL9-pG2wFGsNPmSRAFzHf0_twlGlKW8djNrSRfvNH4yiccNBrrK2AWsEjzzSWDuUjvkN2SIiMneoA4_ZiJ7VEDS48--Lnd9Fa_snTRt34MrLRGblzedCqSBho4XIulH5-bUwN0JBkaUzsrAe4GSN0vKZBH3V7m-Yexpy5T_TjUyEoJnY_agXXRWg5rl5wQj7P7L8O-PR2DSNowZcRdBdvC2UZYUZLXbc1h5YYuM2glP2Hz-cxfUMossjRAooelHnChH3NkYkbtNe2z9YoA"
                    },
                    "expires_in": {
                      "type": "number",
                      "default": 60
                    },
                    "refresh_expires_in": {
                      "type": "number",
                      "default": 1800
                    },
                    "refresh_token": {
                      "type": "string",
                      "default": "eyJhbGciOiJIUzI1NiIsInR5cCIgOiAiSldUIiwia2lkIiA6ICJjZjA0M2EzNy05MmQ3LTQ5OTgtODE0My0wZjg0NTZmM2ExNTQifQ..BMCn4ogkz_0Kjs9AfmSnNyTPxZxMwPUViVt3ttcjRy0"
                    },
                    "token_type": {
                      "type": "string",
                      "default": "Bearer"
                    },
                    "not-before-policy": {
                      "type": "number",
                      "default": 0
                    },
                    "session_state": {
                      "type": "string",
                      "default": "600948f9-46de-4a1f-b9dc-235dc40b2066"
                    },
                    "scope": {
                      "type": "string",
                      "default": "email, profile"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Access Token User Error"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    }
  },
