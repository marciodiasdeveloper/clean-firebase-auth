{
  "name": "madahjs-firebase-auth",
  "version": "0.0.1",
  "description": "Professional API using Clean Architecture and TDD.",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "clean architecture",
    "clean code",
    "tdd",
    "ddd",
    "solid",
    "design patterns",
    "node",
    "typescript",
    "jest"
  ],
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --transpile-only -r dotenv/config -r src/main/server.ts",
    "start": "node -r dotenv/config dist/main/server.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:swc",
    "build:swc": "swc src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "prepare": "husky install",
    "check": "npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand --detectOpenHandles",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:integration": "npm t -- --config ./jest.integration.config.js",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.38.0",
    "@opentelemetry/sdk-node": "^0.41.1",
    "@sentry/node": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "amqplib": "^0.10.3",
    "axios": "^1.2.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "endent": "^2.1.0",
    "express": "^4.18.2",
    "figlet": "^1.5.2",
    "firebase": "^11.0.1",
    "ioredis": "^5.2.5",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^10.0.0",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@jest-mock/express": "^1.4.4",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.0",
    "@swc/helpers": "0.5.12",
    "@swc/jest": "0.2.36",
    "@types/amqplib": "^0.10.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.5",
    "@types/got": "^9.6.12",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.18.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.20",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
